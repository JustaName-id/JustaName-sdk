{
  "name": "@justaname.id/react-signin",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/@justaname.id/react-signin/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "storybook": {
      "executor": "@nx/storybook:storybook",
      "options": {
        "port": 4401,
        "configDir": "packages/@justaname.id/react-signin/.storybook"
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build": {
      "executor": "@nx/rollup:rollup",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "packages/@justaname.id/react-signin/dist",
        "tsConfig": "packages/@justaname.id/react-signin/tsconfig.lib.json",
        "project": "packages/@justaname.id/react-signin/package.json",
        "entryFile": "packages/@justaname.id/react-signin/src/index.ts",
        "external": [
          "react",
          "react-dom",
          "react/jsx-runtime"
        ],
        "rollupConfig": "packages/@justaname.id/react-signin/rollup.config.cjs",
        "generateExportsField": true,
        "compiler": "swc",
        "format": [
          "esm"
        ],
        "assets": [
          {
            "glob": "packages/@justaname.id/react-signin/README.md",
            "input": ".",
            "output": "."
          }
        ]
      },
      "configurations": {
        "production": {
          "optimization": true,
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        }
      },
      "dependsOn": [
        "lint",
        {
          "projects": [
            "@justaname.id/sdk",
            "@justaname.id/react",
            "@justaname.id/react-ui"
          ],
          "target": "build",
          "configuration": "production"
        }
      ]
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "fix": true,
        "lintFilePatterns": [
          "packages/@justaname.id/react/**/*.{ts,tsx,js,jsx}",
          "packages/@justaname.id/react/package.json"
        ]
      }
    }
  }
}
