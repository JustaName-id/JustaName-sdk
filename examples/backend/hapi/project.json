{
  "name": "justaname-hapi",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "examples/backend/hapi/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn build",
        "cwd": "examples/backend/hapi"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn serve",
        "cwd": "examples/backend/hapi"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn lint",
        "cwd": "examples/backend/hapi"
      }
    },
    "install": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn",
        "cwd": "examples/backend/hapi"
      }
    }
  },
  "tags": []
}
